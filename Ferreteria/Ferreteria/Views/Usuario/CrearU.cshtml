@model Ferreteria.Models.Usuario
@{
    ViewBag.Title = "CrearU";
}
<head>
    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
    <script src="~/Scripts/jquery-3.3.1.js"></script>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />

</head>




<h2>Ingresar Nuevo Usuario</h2>

<div class="row">
    <div class="col-lg-6">
        <form class="form-horizontal" action="/Usuario/CrearU" method="post">
            <div class="form-group">
                <div class="col-sm-10">
                    DNI:  <input id="DNI" type="text" class="form-control" name="DNI" />

                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-10">
                    Nombres:  <input id="Nombres" type="text" class="form-control" name="Nombres" />
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-10">
                    Apellidos:  <input id="Apellidos" type="text" class="form-control" name="Apellidos" />
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-10">
                    Telefono:  <input id="Telefono" type="text" class="form-control" name="Telefono" />
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-10">
                    Correo:  <input id="Correo" type="text" class="form-control" name="Correo" />
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-10">
                    Password:  <input id="Password" type="password" class="form-control" name="Password" />
                </div>
            </div>


            <div class="form-group">

                <div class="col-sm-10">
                    <button type="submit" class="btn btn-danger" >Guardar</button>
                </div>
            </div>
        </form>
    </div>


</div>




